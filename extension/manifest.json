{
  "manifest_version": 2,
  "name": "daylight",
  "description": "alpha v0.0.1 | Chrome extension to customise the Apple iCloud web interface - By Marmatee",
  "homepage_url": "https://github.com/Marmatee/daylight",
  "version": "0.0.3",
  "browser_action": {
    "default_title": "daylight"
  },
  "permissions": [
          "storage"
        ],
  "content_scripts": [
    {
      "matches": ["https://www.icloud.com/"],
      "css": ["css/icloud.css", "css/menu.css"],
      "js": ["js/jquery-2.2.4.js", "js/engine.js"]
    }
  ]
}
